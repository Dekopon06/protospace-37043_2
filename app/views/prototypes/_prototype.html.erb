<div class="card">
  <%= link_to image_tag(prototype.image), root_path, class: :card__img %>
  <%# <%= link_to if @prototype.image.attached?("image", class: :card__img), root_path%>
  <%# <%=link_to image_tag @prototype.image, class: '' if @prototype.image.attached? %>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by {prototypes.use_name}", root_path, class: :card__user %>
  </div>
</div>